# Database Configuration (Required)
DATABASE_URL="postgresql://username:password@localhost:5432/database_name"
DIRECT_URL="postgresql://username:password@localhost:5432/database_name"

# Application Environment
NODE_ENV="development"
LOG_LEVEL="info"

# Supabase Configuration
SUPABASE_PAID_TIER="false"

# Seeding Configuration (Optional - defaults provided)
BATCH_SIZE="25"
PARALLEL_BATCHES="2"
TRANSACTION_BATCH_SIZE="50"
BATCH_DELAY_MS="100"

# Feature Flags (Optional - defaults provided)
LOG_ERRORS="true"
ENABLE_PROGRESS_TRACKING="true"

# Seeding Control Flags (Optional - set to false to skip sections)
SEED_PROGRAM_AREAS="true"
SEED_PROGRAMS="true"
SEED_NOC_UNIT_GROUPS="true"
SEED_OUTLOOKS="true"
SEED_PROGRAM_NOC_LINKS="true"

# File Paths (Optional - defaults provided)
PROGRESS_FILE="seeding-progress.json"
DATA_DIR="data" 
